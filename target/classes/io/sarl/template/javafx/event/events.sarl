package io.sarl.template.javafx.^event

import io.sarl.template.javafx.Model.Road
import java.util.UUID

/** 
 * @author Luka Lambalot
 * 
 */
event Perception{
	
	var road : Road
	
	new (road : Road){
		this.road = road
	}
}

event ReturnPerception{
	var i : int
	var id : UUID
	var nextCoordFree: boolean
	
	new (i : int, id : UUID, nextCoordFree : boolean){
		this.i = i
		this.id = id
		this.nextCoordFree = nextCoordFree
	}
	
}
